      <CodeComponent name="famil_code" plugin="None">
        <Param val="" valType="extendedCode" updates="constant" name="Before Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Before JS Experiment"/>
        <Param val="famil_selected = &quot;&quot;&amp;#10;famil_answer = 0" valType="extendedCode" updates="constant" name="Begin Experiment"/>
        <Param val="famil_selected = &quot;&quot;;&amp;#10;famil_answer = 0;" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
        <Param val="// show mouse&amp;#10;document.body.style.cursor = 'auto';&amp;#10;&amp;#10;// show response options (lookup from window globals created earlier)&amp;#10;function lookup_global(name) {&amp;#10;  name = String(name).trim();&amp;#10;  if (!(name in window)) {&amp;#10;    throw new Error(&quot;Message key not found on window: &quot; + name);&amp;#10;  }&amp;#10;  return window[name];&amp;#10;}&amp;#10;&amp;#10;famil_optA_msg = lookup_global(famil_optionA);&amp;#10;famil_optB_msg = lookup_global(famil_optionB);&amp;#10;famil_optC_msg = lookup_global(famil_optionC);&amp;#10;famil_optD_msg = lookup_global(famil_optionD);&amp;#10;&amp;#10;// text alignment&amp;#10;famil_instr_txt_2.anchor = 'left';&amp;#10;famil_optA.anchor = 'left';&amp;#10;famil_optB.anchor = 'left';&amp;#10;famil_optC.anchor = 'left';&amp;#10;famil_optD.anchor = 'left';&amp;#10;&amp;#10;// reset visuals&amp;#10;familpoly_A.fillColor = &quot;white&quot;;&amp;#10;familpoly_B.fillColor = &quot;white&quot;;&amp;#10;familpoly_C.fillColor = &quot;white&quot;;&amp;#10;familpoly_D.fillColor = &quot;white&quot;;&amp;#10;&amp;#10;// response bookkeeping&amp;#10;famil_selected = null;&amp;#10;tSelected = null;&amp;#10;&amp;#10;// reset mouse click state&amp;#10;famil_mouse.clickReset();&amp;#10;&amp;#10;// helper for audio in feedback&amp;#10;announce_solution_audio = &quot;fpjt_audios/solution_&quot; + String(lang_code) + &quot;.mp3&quot;;&amp;#10;famil_solution_audio = &quot;fpjt_audios/&quot; + String(famil_solution) + &quot;_&quot; + String(lang_code) + &quot;.mp3&quot;;&amp;#10;&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="# show response options&amp;#10;def lookup_global(name):&amp;#10;    name = str(name).strip()&amp;#10;    return globals().get(name, name)&amp;#10;&amp;#10;famil_optA_msg = lookup_global(famil_optionA)&amp;#10;famil_optB_msg = lookup_global(famil_optionB)&amp;#10;famil_optC_msg = lookup_global(famil_optionC)&amp;#10;famil_optD_msg = lookup_global(famil_optionD)&amp;#10;&amp;#10;# text alignment&amp;#10;famil_instr_txt_2.alignText = 'left'&amp;#10;famil_optA.alignText = 'left'&amp;#10;famil_optB.alignText = 'left'&amp;#10;famil_optC.alignText = 'left'&amp;#10;famil_optD.alignText = 'left'&amp;#10;&amp;#10;# reset visuals&amp;#10;familpoly_A.fillColor = &quot;white&quot;&amp;#10;familpoly_B.fillColor = &quot;white&quot;&amp;#10;familpoly_C.fillColor = &quot;white&quot;&amp;#10;familpoly_D.fillColor = &quot;white&quot;&amp;#10;&amp;#10;# response bookkeeping&amp;#10;famil_selected = None&amp;#10;tSelected = None&amp;#10;&amp;#10;# reset mouse click state (prevents carry-over click)&amp;#10;famil_mouse.clickReset()&amp;#10;&amp;#10;# helper for audio in feedback&amp;#10;announce_solution_audio = &quot;fpjt_audios/solution_&quot; + str(lang_code) + &quot;.mp3&quot;&amp;#10;famil_solution_audio = &quot;fpjt_audios/&quot; + str(famil_solution) + &quot;_&quot; + str(lang_code) + &quot;.mp3&quot;&amp;#10;" valType="extendedCode" updates="constant" name="Begin Routine"/>
        <Param val="Both" valType="str" updates="None" name="Code Type"/>
        <Param val="# show mouse after 200ms&amp;#10;if t &gt;= 0.2:&amp;#10;    win.mouseVisible = True&amp;#10;&amp;#10;# evaluate click&amp;#10;if famil_selected is None:&amp;#10;    buttons = famil_mouse.getPressed()  # [left, middle, right]&amp;#10;    if buttons[0]:  # left click&amp;#10;        if famil_mouse.isPressedIn(familpoly_A):&amp;#10;            famil_selected = &quot;A&quot;&amp;#10;            familpoly_A.fillColor = &quot;black&quot;&amp;#10;            tSelected = t&amp;#10;        elif famil_mouse.isPressedIn(familpoly_B):&amp;#10;            famil_selected = &quot;B&quot;&amp;#10;            familpoly_B.fillColor = &quot;black&quot;&amp;#10;            tSelected = t&amp;#10;        elif famil_mouse.isPressedIn(familpoly_C):&amp;#10;            famil_selected = &quot;C&quot;&amp;#10;            familpoly_C.fillColor = &quot;black&quot;&amp;#10;            tSelected = t&amp;#10;        elif famil_mouse.isPressedIn(familpoly_D):&amp;#10;            famil_selected = &quot;D&quot;&amp;#10;            familpoly_D.fillColor = &quot;black&quot;&amp;#10;            tSelected = t&amp;#10;else:&amp;#10;    # wait 500ms with black circle, then advance&amp;#10;    if (t - tSelected) &gt;= 0.5:&amp;#10;        continueRoutine = False&amp;#10;" valType="extendedCode" updates="constant" name="Each Frame"/>
        <Param val="// show mouse&amp;#10;if (t &gt;= 0.2) {&amp;#10;  document.body.style.cursor = 'auto';&amp;#10;}&amp;#10;&amp;#10;// evaluate click&amp;#10;if (famil_selected === null) {&amp;#10;  let buttons = famil_mouse.getPressed(); // [left, middle, right]&amp;#10;  if (buttons[0]) { // left click&amp;#10;    if (famil_mouse.isPressedIn(familpoly_A)) {&amp;#10;      famil_selected = &quot;A&quot;;&amp;#10;      familpoly_A.fillColor = &quot;black&quot;;&amp;#10;      tSelected = t;&amp;#10;    } else if (famil_mouse.isPressedIn(familpoly_B)) {&amp;#10;      famil_selected = &quot;B&quot;;&amp;#10;      familpoly_B.fillColor = &quot;black&quot;;&amp;#10;      tSelected = t;&amp;#10;    } else if (famil_mouse.isPressedIn(familpoly_C)) {&amp;#10;      famil_selected = &quot;C&quot;;&amp;#10;      familpoly_C.fillColor = &quot;black&quot;;&amp;#10;      tSelected = t;&amp;#10;    } else if (famil_mouse.isPressedIn(familpoly_D)) {&amp;#10;      famil_selected = &quot;D&quot;;&amp;#10;      familpoly_D.fillColor = &quot;black&quot;;&amp;#10;      tSelected = t;&amp;#10;    }&amp;#10;  }&amp;#10;} else {&amp;#10;  // wait 500ms with black circle, then advance&amp;#10;  if ((t - tSelected) &gt;= 0.5) {&amp;#10;    continueRoutine = false;&amp;#10;  }&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="Each JS Frame"/>
        <Param val="" valType="extendedCode" updates="constant" name="End Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="End JS Experiment"/>
        <Param val="// correctness (0/1)&amp;#10;famil_answer = (famil_selected === famil_option_solution) ? 1 : 0;&amp;#10;&amp;#10;if (famil_answer === 1) {&amp;#10;  famil_feedback_msg = feedback_correct_msg;&amp;#10;} else if (famil_answer === 0) {&amp;#10;  famil_feedback_msg = feedback_incorrect_msg;&amp;#10;}&amp;#10;&amp;#10;// save data&amp;#10;psychoJS.experiment.addData(&quot;famil_selected&quot;, famil_selected);&amp;#10;psychoJS.experiment.addData(&quot;famil_answer&quot;, famil_answer);&amp;#10;&amp;#10;// hide mouse&amp;#10;document.body.style.cursor = 'none';" valType="extendedCode" updates="constant" name="End JS Routine"/>
        <Param val="# correctness (0/1)&amp;#10;famil_answer = int(famil_selected == famil_option_solution)&amp;#10;&amp;#10;if famil_answer == 1: &amp;#10;    famil_feedback_msg = feedback_correct_msg &amp;#10;elif famil_answer == 0:&amp;#10;    famil_feedback_msg = feedback_incorrect_msg &amp;#10;&amp;#10;# save data&amp;#10;thisExp.addData(&quot;famil_selected&quot;, famil_selected)&amp;#10;thisExp.addData(&quot;famil_answer&quot;, famil_answer)&amp;#10;&amp;#10;# hide mouse&amp;#10;win.mouseVisible = False&amp;#10;" valType="extendedCode" updates="constant" name="End Routine"/>
        <Param val="False" valType="bool" updates="None" name="disabled"/>
