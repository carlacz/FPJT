: 'C'}, {'famil_image': 'fpjt_images/torso_right.jpg', 'famil_optionA': 'torso_right', 'famil_optionB': 'head_up', 'famil_optionC': 'torso_left', 'famil_optionD': 'no_movement', 'famil_solution': 'torso_right', 'famil_option_solution': 'A'}, {'famil_image': 'fpjt_images/torso_left.jpg', 'famil_optionA': 'torso_right', 'famil_optionB': 'head_down', 'famil_optionC': 'torso_left', 'famil_optionD': 'no_movement', 'famil_solution': 'torso_left', 'famil_option_solution': 'C'}, {'famil_image': 'fpjt_images/right_leg_front.jpg', 'famil_optionA': 'right_leg_front', 'famil_optionB': 'right_leg_back', 'famil_optionC': 'left_leg_front', 'famil_optionD': 'no_movement', 'famil_solution': 'right_leg_front', 'famil_option_solution': 'A'}, {'famil_image': 'fpjt_images/right_leg_side.jpg', 'famil_optionA': 'right_leg_front', 'famil_optionB': 'left_leg_side', 'famil_optionC': 'right_leg_side', 'famil_optionD': 'no_movement', 'famil_solution': 'right_leg_side', 'famil_option_solution': 'C'}, {'famil_image': 'fpjt_images/right_leg_back.jpg', 'famil_optionA': 'right_leg_back', 'famil_optionB': 'left_leg_back', 'famil_optionC': 'right_leg_front', 'famil_optionD': 'no_movement', 'famil_solution': 'right_leg_back', 'famil_option_solution': 'A'}, {'famil_image': 'fpjt_images/left_leg_front.jpg', 'famil_optionA': 'right_leg_front', 'famil_optionB': 'left_leg_front', 'famil_optionC': 'right_leg_back', 'famil_optionD': 'no_movement', 'famil_solution': 'left_leg_front', 'famil_option_solution': 'B'}, {'famil_image': 'fpjt_images/left_leg_side.jpg', 'famil_optionA': 'left_leg_back', 'famil_optionB': 'right_leg_back', 'famil_optionC': 'left_leg_side', 'famil_optionD': 'no_movement', 'famil_solution': 'left_leg_side', 'famil_option_solution': 'C'}, {'famil_image': 'fpjt_images/left_leg_back.jpg', 'famil_optionA': 'left_leg_back', 'famil_optionB': 'right_leg_back', 'famil_optionC': 'left_leg_front', 'famil_optionD': 'no_movement', 'famil_solution': 'left_leg_back', 'famil_option_solution': 'A'}]" va