      <CodeComponent name="code" plugin="None">
        <Param val="" valType="extendedCode" updates="constant" name="Before Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Before JS Experiment"/>
        <Param val="# initialize variables&amp;#10;CC_answer = 0&amp;#10;CC_error_msg = &quot;&quot;&amp;#10;CC_feedback_msg = &quot;&quot;&amp;#10;CC_attempt = 0" valType="extendedCode" updates="constant" name="Begin Experiment"/>
        <Param val="// initialize variables&amp;#10;CC_answer = 0;&amp;#10;CC_error_msg = &quot;&quot;;&amp;#10;CC_feedback_msg = &quot;&quot;;&amp;#10;CC_attempt = 0;&amp;#10;continueLoop = 1;&amp;#10;// hide mouse&amp;#10;document.body.style.cursor = 'none';" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
        <Param val="CC_instr_txt_1.anchor = 'left';&amp;#10;CC_instr_txt_2.anchor = 'left';&amp;#10;CC_option_A_txt.anchor = 'left';&amp;#10;CC_option_B_txt.anchor = 'left';&amp;#10;CC_option_C_txt.anchor = 'left';&amp;#10;CC_option_D_txt.anchor = 'left';&amp;#10;CC_option_E_txt.anchor = 'left';&amp;#10;CC_option_F_txt.anchor = 'left';&amp;#10;&amp;#10;// state: 0 = white; 1 = black&amp;#10;poly_A_state = 0;&amp;#10;poly_B_state = 0;&amp;#10;poly_C_state = 0;&amp;#10;poly_D_state = 0;&amp;#10;poly_E_state = 0;&amp;#10;poly_F_state = 0;&amp;#10;&amp;#10;// left mouse button pressed in last frame?&amp;#10;_prevLeft = 0;&amp;#10;&amp;#10;// show mouse&amp;#10;document.body.style.cursor = 'auto';" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="CC_instr_txt_1.alignText = 'left'&amp;#10;CC_instr_txt_2.alignText = 'left'&amp;#10;CC_option_A_txt.alignText = 'left'&amp;#10;CC_option_B_txt.alignText = 'left'&amp;#10;CC_option_C_txt.alignText = 'left'&amp;#10;CC_option_D_txt.alignText = 'left'&amp;#10;CC_option_E_txt.alignText = 'left'&amp;#10;CC_option_F_txt.alignText = 'left'&amp;#10;&amp;#10;# state: 0 = white (not selected), 1 = black (selected)&amp;#10;poly_A_state = 0&amp;#10;poly_B_state = 0&amp;#10;poly_C_state = 0&amp;#10;poly_D_state = 0&amp;#10;poly_E_state = 0&amp;#10;poly_F_state = 0&amp;#10;&amp;#10;# Remember whether the left mouse button was pressed in the last frame&amp;#10;_prevLeft = 0&amp;#10;&amp;#10;# show mouse&amp;#10;win.mouseVisible = True" valType="extendedCode" updates="constant" name="Begin Routine"/>
        <Param val="Both" valType="str" updates="None" name="Code Type"/>
        <Param val="left = mouse.getPressed()[0]&amp;#10;&amp;#10;# Toggle nur beim &quot;neu gedrückten&quot; Klick (nicht beim Gedrückthalten)&amp;#10;if left and not _prevLeft:&amp;#10;    if mouse.isPressedIn(poly_A):&amp;#10;        poly_A_state = 1 - poly_A_state&amp;#10;    if mouse.isPressedIn(poly_B):&amp;#10;        poly_B_state = 1 - poly_B_state&amp;#10;    if mouse.isPressedIn(poly_C):&amp;#10;        poly_C_state = 1 - poly_C_state&amp;#10;    if mouse.isPressedIn(poly_D):&amp;#10;        poly_D_state = 1 - poly_D_state&amp;#10;    if mouse.isPressedIn(poly_E):&amp;#10;        poly_E_state = 1 - poly_E_state&amp;#10;    if mouse.isPressedIn(poly_F):&amp;#10;        poly_F_state = 1 - poly_F_state&amp;#10;&amp;#10;# Darstellung aus den Zuständen ableiten&amp;#10;poly_A.setFillColor('black' if poly_A_state else 'white')&amp;#10;poly_B.setFillColor('black' if poly_B_state else 'white')&amp;#10;poly_C.setFillColor('black' if poly_C_state else 'white')&amp;#10;poly_D.setFillColor('black' if poly_D_state else 'white')&amp;#10;poly_E.setFillColor('black' if poly_E_state else 'white')&amp;#10;poly_F.setFillColor('black' if poly_F_state else 'white')&amp;#10;&amp;#10;_prevLeft = left" valType="extendedCode" updates="constant" name="Each Frame"/>
        <Param val="// linker Mausbutton (0 = nicht gedrückt, 1 = gedrückt)&amp;#10;let left = mouse.getPressed()[0];&amp;#10;&amp;#10;// Toggle nur beim NEUEN Klick&amp;#10;if (left &amp;&amp; !_prevLeft) {&amp;#10;&amp;#10;    if (mouse.isPressedIn(poly_A)) {&amp;#10;        poly_A_state = 1 - poly_A_state;&amp;#10;    }&amp;#10;    if (mouse.isPressedIn(poly_B)) {&amp;#10;        poly_B_state = 1 - poly_B_state;&amp;#10;    }&amp;#10;    if (mouse.isPressedIn(poly_C)) {&amp;#10;        poly_C_state = 1 - poly_C_state;&amp;#10;    }&amp;#10;    if (mouse.isPressedIn(poly_D)) {&amp;#10;        poly_D_state = 1 - poly_D_state;&amp;#10;    }&amp;#10;    if (mouse.isPressedIn(poly_E)) {&amp;#10;        poly_E_state = 1 - poly_E_state;&amp;#10;    }&amp;#10;    if (mouse.isPressedIn(poly_F)) {&amp;#10;        poly_F_state = 1 - poly_F_state;&amp;#10;    }&amp;#10;}&amp;#10;&amp;#10;// Darstellung aus Zustand ableiten&amp;#10;poly_A.setFillColor(poly_A_state ? 'black' : 'white');&amp;#10;poly_B.setFillColor(poly_B_state ? 'black' : 'white');&amp;#10;poly_C.setFillColor(poly_C_state ? 'black' : 'white');&amp;#10;poly_D.setFillColor(poly_D_state ? 'black' : 'white');&amp;#10;poly_E.setFillColor(poly_E_state ? 'black' : 'white');&amp;#10;poly_F.setFillColor(poly_F_state ? 'black' : 'white');&amp;#10;&amp;#10;// aktuellen Zustand merken&amp;#10;_prevLeft = left;" valType="extendedCode" updates="constant" name="Each JS Frame"/>
        <Param val="" valType="extendedCode" updates="constant" name="End Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="End JS Experiment"/>
        <Param val="// 1 = schwarz ausgewählt, 0 = weiß nicht ausgewählt&amp;#10;poly_A_clicked = poly_A_state;&amp;#10;poly_B_clicked = poly_B_state;&amp;#10;poly_C_clicked = poly_C_state;&amp;#10;poly_D_clicked = poly_D_state;&amp;#10;poly_E_clicked = poly_E_state;&amp;#10;poly_F_clicked = poly_F_state;&amp;#10;&amp;#10;// evaluate answer&amp;#10;CC_answer = (&amp;#10;  poly_C_state === 1 &amp;&amp;&amp;#10;  poly_D_state === 1 &amp;&amp;&amp;#10;  poly_F_state === 1&amp;#10;) ? 1 : 0;&amp;#10;&amp;#10;if ((CC_answer === 1)) {&amp;#10;    CC_feedback_msg = feedback_correct_msg;&amp;#10;    CC_error_msg = &quot;&quot;;&amp;#10;    CC_loop.finished = true;&amp;#10;} else {&amp;#10;    if ((CC_answer === 0)) {&amp;#10;        CC_feedback_msg = feedback_incorrect_msg;&amp;#10;        CC_error_msg = CC_error;&amp;#10;        CC_loop.finished = false;&amp;#10;    }&amp;#10;}&amp;#10;&amp;#10;// count attempts&amp;#10;CC_attempt += 1&amp;#10;&amp;#10;// show mouse&amp;#10;document.body.style.cursor = 'none';&amp;#10;&amp;#10;// log&amp;#10;psychoJS.experiment.addData('poly_A_state', poly_A_state);&amp;#10;psychoJS.experiment.addData('poly_B_state', poly_B_state);&amp;#10;psychoJS.experiment.addData('poly_C_state', poly_C_state);&amp;#10;psychoJS.experiment.addData('poly_D_state', poly_D_state);&amp;#10;psychoJS.experiment.addData('poly_E_state', poly_E_state);&amp;#10;psychoJS.experiment.addData('poly_F_state', poly_F_state);&amp;#10;psychoJS.experiment.addData('CC_answer', CC_answer);&amp;#10;psychoJS.experiment.addData('CC_attempt', CC_attempt);" valType="extendedCode" updates="constant" name="End JS Routine"/>
        <Param val="# Genau das, was du brauchst: 1 wenn am Ende schwarz, sonst 0&amp;#10;poly_A_clicked = int(poly_A_state)&amp;#10;poly_B_clicked = int(poly_B_state)&amp;#10;poly_C_clicked = int(poly_C_state)&amp;#10;poly_D_clicked = int(poly_D_state)&amp;#10;poly_E_clicked = int(poly_E_state)&amp;#10;poly_F_clicked = int(poly_F_state)&amp;#10;&amp;#10;# evaluate answer&amp;#10;CC_answer = 1 if (poly_C_state == 1 and poly_D_state == 1 and poly_F_state == 1) else 0&amp;#10;&amp;#10;if CC_answer == 1: &amp;#10;    CC_feedback_msg = feedback_correct_msg &amp;#10;    CC_error_msg = &quot;&quot;&amp;#10;    CC_loop.finished = True&amp;#10;elif CC_answer == 0:&amp;#10;    CC_feedback_msg = feedback_incorrect_msg &amp;#10;    CC_error_msg = CC_error&amp;#10;    CC_loop.finished = False&amp;#10;&amp;#10;# count attempts&amp;#10;CC_attempt += 1&amp;#10;&amp;#10;# show mouse&amp;#10;win.mouseVisible = False&amp;#10;&amp;#10;# log&amp;#10;thisExp.addData('poly_A_state', poly_A_state)&amp;#10;thisExp.addData('poly_B_state', poly_B_state)&amp;#10;thisExp.addData('poly_C_state', poly_C_state)&amp;#10;thisExp.addData('poly_D_state', poly_D_state)&amp;#10;thisExp.addData('poly_E_state', poly_E_state)&amp;#10;thisExp.addData('poly_F_state', poly_F_state)&amp;#10;thisExp.addData('CC_answer', CC_answer)&amp;#10;thisExp.addData('CC_attempt', CC_attempt)" valType="extendedCode" updates="constant" name="End Routine"/>
        <Param val="False" valType="bool" updates="None" name="disabled"/>
        <Param val="code" valType="code" updates="None" name="name"/>
