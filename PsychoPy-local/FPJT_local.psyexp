        <Param val="// change code that does not auto-translate correctly&amp;#10;// we use window instead of globals()&amp;#10;// to create the global variables&amp;#10;// the rest is the same&amp;#10;&amp;#10;let row = update_message_loop.getCurrentTrial();&amp;#10;let msg_name = String(row[&quot;message&quot;]).trim(); &amp;#10;let msg_value = String(row[lang_code]);       &amp;#10;&amp;#10;window[msg_name] = msg_value;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
